<odoo>
    <data>

    <record id="view_stock_return_picking_form" model="ir.ui.view">
        <field name="name">Return lines</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form"></field>
        <field name="arch" type="xml">
                           
                            <button name="create_returns" position="attributes">
                            	<attribute name="context">{'huy_bien_ban':context.get('huy_bien_ban')}</attribute>
                            </button>
                          
                            <xpath expr="//field[@name='product_return_moves']/tree/field[@name='uom_id']" position="after">
                            	<field name="ml_id"></field>
                            </xpath>
        			<field name="product_return_moves" position="attributes">
        				<attribute name="readonly">context.get('huy_bien_ban')</attribute>
        			</field>
                    
                    
                    <field name="parent_location_id" position="attributes">
                    	<attribute name="invisible">0</attribute>
                    </field>
                    <!--  
                      <field name="original_location_id" position="attributes">
                    	<attribute name="invisible">0</attribute>
                    </field>
                    -->
                     <field name="parent_location_id" position="after">
                    	<!-- <field name="is_chuyen_tiep"></field> -->
                    	<field name="loai_tra_hay_chuyen_tiep"></field>
                    </field>
		</field>
	</record>
</data>
</odoo>