<odoo>
<data>


<record id="downloadquants_form" model="ir.ui.view">
            <field name="name">downloadwizard.download.form</field>
            <field name="model">downloadwizard.download</field>
            <field name="inherit_id" ref="downloadwizard.downloadquants_form"></field>
            <field name="arch" type="xml">
            	<field name="is_moi_sheet_moi_loai" position="before">
            		<field name="date" attrs="{'invisible':[('model_name','!=','download_bcn'),('model_name','!=','download_p3')]}"></field>
            		<field name="end_date" attrs="{'invisible':[('model_name','!=','download_p3')]}"></field>
            		<field name="chon_thang" attrs="{'invisible':[('model_name','!=','download_p3')]}"></field>
            		<field name="department_id" attrs="{'invisible':[('model_name','!=','download_p3')]}"></field>
            		<field name="chi_tiet_hay_danh_sach" attrs="{'invisible':[('model_name','!=','download_p3')]}"></field>
            	</field>
            </field>
            </record>   
	 <act_window id="download_tvcv" name="Download TVCV" res_model="downloadwizard.download" src_model="tvcv" view_mode="form" multi="True" target="new" key2="client_action_multi" />
       <act_window id="download_user" name="Download Danh sách Users" res_model="downloadwizard.download" src_model="res.users" view_mode="form" multi="True" target="new" key2="client_action_multi" />
       <act_window id="download_bcn" name="Download Báo cáo ngày" res_model="downloadwizard.download" src_model="ctr" view_mode="form" multi="True" target="new" key2="client_action_multi" context="{'transfer_active_model':'download_bcn'}" />
       <act_window id="download_cvi" name="Download list Công việc/ sự cố/sự vụ" res_model="downloadwizard.download" src_model="cvi" view_mode="form" multi="True" target="new" key2="client_action_multi"  />

<menuitem id="dlcv_menu" parent="else_more_menu" action="dlcv_action" sequence="9" />
 <menuitem name="Download Báo cáo ngày" id="dl_bcn_2" parent="else_more_menu" action="downloadwizard.downloadwizard_action" sequence="8" /> 


<record model="ir.actions.act_window" id="downloadp3_action">
         <field name="name">Download </field>
         <field name="res_model">downloadwizard.download</field>
         <field name="view_mode">form</field>
         <field name="view_type">form</field>
         <field name="context">{'transfer_active_model':'download_p3'}</field>
         <field name="target">new</field>
      </record>
      
 <menuitem name="Download P3" id="dl_p3" parent="else_more_menu" action="downloadp3_action" sequence="8" /> 


</data>
</odoo>