       <act_window id="download_bcn" name="Download BCN" res_model="downloadwizard.download" src_model="ctr" view_mode="form" multi="True" target="new" key2="client_action_multi" context="{'transfer_active_model':'download_bcn'}" />

       <act_window id="download_cvi" name="Download list theo domain" res_model="downloadwizard.download" src_model="cvi" view_mode="form" multi="True" target="new" key2="client_action_multi"  />

